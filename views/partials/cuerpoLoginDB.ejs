    <div class="etiquetasDeLogin">
        <p><label class="usuario"for ="usuario">Usuario</label>
            <input id="user"placeholder="" type="text"  name="usuario" class="barraDeUsuario" ></p>
            <% if(locals.errorsLogin && locals.errorsLogin.usuario){ %>
                <span id="errorMsge" > <%= errorsLogin.usuario.msg  %> </span>
              <% } %> 
            <br/>

        <p><label class="usuario"for ="contraseña">Contraseña</label>
            <input id="pass" placeholder="" type="password"  name="contraseña" class="barraDeUsuario" ></p>
            <% if(locals.errorsLogin && locals.errorsLogin.contraseña){ %>
                <span id="errorMsge" > <%= errorsLogin.contraseña.msg  %> </span>
            <% } %> 
            <br/>
            <label for="recordame" class="usuario">Recordame</label>
            <input type="checkbox" name="recordame" >
    </div>
        